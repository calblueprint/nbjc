generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model migrations {
  id     Int      @default(autoincrement()) @id
  name   String
  run_on DateTime
}

model organzation {
  id   Int     @default(autoincrement()) @id
  name String
  long Int?
  lat  Int?
  type String?
}
