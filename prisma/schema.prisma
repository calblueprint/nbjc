generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model migrations {
  id     Int      @id @default(autoincrement())
  name   String
  run_on DateTime
}

model organization {
  id         Int      @id @default(autoincrement())
  name       String?
  lat        Int?
  long       Int?
  type       String?
  many_users Boolean?
  one_app    Boolean?
}
