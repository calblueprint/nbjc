generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model migrations {
  id     Int      @id @default(autoincrement())
  name   String
  run_on DateTime
}

model orgnizations {
  id   Int     @id @default(autoincrement())
  name String
  long Float?
  lat  Float?
  type String?
}
